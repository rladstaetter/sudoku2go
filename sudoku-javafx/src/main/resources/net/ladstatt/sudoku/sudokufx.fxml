<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="656.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.ladstatt.sudoku.SudokuFXController">
    <top>
        <ToolBar fx:id="digitToolBar" prefHeight="80.0" visible="false" />
    </top>
    <center>
        <ImageView fx:id="videoView" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="TOP_CENTER" />
    </center>
    <right>
        <VBox BorderPane.alignment="TOP_CENTER">
            <ImageView fx:id="normalizedView" fitWidth="500.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="solutionView" fitWidth="500.0" pickOnBounds="true" preserveRatio="true" />
        </VBox>
    </right>
</BorderPane>
